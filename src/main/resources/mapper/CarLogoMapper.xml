<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ecar.dao.CarLogoDAO">


  <select id="listCarLogo" resultType="CarLogo" >
    SELECT "logoId", "logoName", "logoAlphabet", "logoUrl"
    FROM tb_carLogo
    ORDER BY "logoAlphabet"
  </select>



</mapper>